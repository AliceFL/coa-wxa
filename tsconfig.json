{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "target": "es5",
    "outDir": "dist",
    "lib": [
      "es5",
      "es2015",
      "es2016",
      "es2017",
      "es2018",
      "es2019",
      "es2020",
      "esnext"
    ],
    "sourceMap": false,
    "declaration": true,
    "importHelpers": true,
    "removeComments": true,
    "types": [
      "miniprogram-api-typings"
    ]
  },
  "include": [
    "src"
  ]
}
